{
  "name": "cinifob-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint --ignore-pattern 'src/generated/**' src/",
    "lint:fix": "eslint --ignore-pattern 'src/generated/**' src/ --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:run": "./run-e2e-tests.sh",
    "test:all": "npm run test -- --run && npm run test:e2e",
    "typecheck": "tsc --noEmit",
    "postinstall": "prisma generate && lefthook install && playwright install",
    "prepare": "lefthook install",
    "db:cleanup": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/cleanup-database.ts",
    "db:seed:genres": "npx ts-node --compiler-options '{\"module\":\"commonjs\"}' scripts/seed-genres.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@iconify/react": "^6.0.2",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "7.0.0-beta.17",
    "@mui/material": "^7.3.2",
    "@mui/x-charts": "^8.12.0",
    "@mui/x-data-grid": "^8.12.1",
    "@mui/x-date-pickers": "^8.12.0",
    "@prisma/client": "^6.16.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-select": "^5.0.1",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "next": "15.5.4",
    "next-auth": "^4.24.11",
    "next-pwa": "^5.6.0",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "react-redux": "^9.2.0",
    "react-select": "^5.10.2",
    "styled-jsx": "^5.1.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "axe-playwright": "^2.2.2",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lefthook": "^1.13.4",
    "lint-staged": "^16.2.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}
